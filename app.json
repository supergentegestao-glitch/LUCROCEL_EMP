{
  "expo": {
    "name": "SuperGente +LUCRO",
    "slug": "supergente-lucro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo4.jpg",
    "scheme": "supergentelucro",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.supergente.lucro",
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "Necessário para acessar conteúdo local",
        "NSBonjourServiceTypes": [
          "_http._tcp",
          "_https._tcp"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/logo4.jpg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.supergente.lucro",
      "versionCode": 1,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/logo4.jpg"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "supergente-lucro"
      }
    }
  }
}
